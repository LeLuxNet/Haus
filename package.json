    "node-telegram-bot-api": "^0.51.0",
    "sharp": "^0.27.0",
    "suncalc": "^1.8.0",
    "tr-O64": "^0.2.4",
    "wallpaper": "^4.4.2",
    "websocket": "^1.0.33"
  },
  "devDependencies": {
    "@types/discord-rpc": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
